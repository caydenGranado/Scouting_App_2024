{
    "builds": [{
        "src": "scouting_backend/scouting_backend/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "scouting_backend/scouting_backend/wsgi.py"
        }
    ]
}